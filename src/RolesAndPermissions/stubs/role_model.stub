<?php

namespace {{ namespace }};

use Illuminate\Database\Eloquent\Factories\HasFactory;
use Illuminate\Database\Eloquent\Model;

class Role extends Model
{
    use HasFactory;
    
    protected $fillable = [
        'name',
    ];

    protected $with = [
        'permissions'
    ];

    public function permissions(){
        return $this->belongsToMany(Permission::class, 'roles_to_permissions');
    }
}
